  FUNCTION GET_PARENTID(ELMCODE IN VARCHAR2) RETURN VARCHAR2;
  FUNCTION NORMALIZE_VALUE(HOMEVAL IN NUMBER, DUALVAL IN NUMBER, HOMECUR IN VARCHAR, NORMCUR IN VARCHAR2) RETURN NUMBER;
