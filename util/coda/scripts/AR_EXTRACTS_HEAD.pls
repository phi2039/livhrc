  PROCEDURE SETTLED_ITEMS_DAYS(P_START_DATE DATE, P_DAYS INT, SETTLED_ITEM_CUR OUT SYS_REFCURSOR);
  PROCEDURE SETTLED_ITEMS_RANGE(P_START_DATE DATE, P_END_DATE DATE, SETTLED_ITEM_CUR OUT SYS_REFCURSOR);
END AR_EXTRACTS;