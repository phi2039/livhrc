  PROCEDURE OPEN_ITEMS_FULL(OPEN_ITEM_CUR OUT SYS_REFCURSOR);
  PROCEDURE OPEN_ITEMS_DELTA(P_START_DATE DATE, P_DAYS INT, OPEN_ITEM_CUR OUT SYS_REFCURSOR);
  PROCEDURE AR_ITEMS_HISTORY(P_START_DATE DATE, P_DAYS INT, AR_ITEM_CUR OUT SYS_REFCURSOR);
